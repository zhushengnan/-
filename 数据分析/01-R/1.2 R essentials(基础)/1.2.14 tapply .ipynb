   "outputs": [],
   "source": [
    "对向量的子集进行操作"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "tapply（向量，因子/因子列表，函数/函数名）"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "x <-c(rnorm(5),runif(5),rnorm(5,1))  #rnorm正态分布，runif均匀分布\n",
    "f <- gl(3,5)   #创建因子，因子有3个水平，每个水平下有5个元素\n",
    "tapply(x,f,mean)  #向量x按照因子的水平进行分组，对每一组求均值"
   ]
  },
  {
